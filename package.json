{
  "name": "api-expressjs-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "build": "yarn clean && tsc && yarn copy-static",
    "copy-static": "mkdir -p ./dist/public && cp ./src/public/* ./dist/public/",
    "clean": "rimraf dist",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "db:generate": "npm run typeorm migration:generate -- -n migrationfile",
    "db:migrate": "npm run db:drop && npm run typeorm migration:run",
    "db:revert": "npm run typeorm migration:revert",
    "db:drop": "npm run typeorm schema:drop",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "dev": "./node_modules/.bin/concurrently \"tsc -w\" \"nodemon\"",
    "start": "node dist/server.js",
    "start:dev": "node dist/server.js"
  },
  "author": "feriva22 <https://github.com/feriva22>",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "google-auth-library": "^6.1.4",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0-alpha.6",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.29",
    "typeorm-seeding": "^1.6.1",
    "typeorm-typedi-extensions": "^0.2.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.9",
    "@types/faker": "^5.1.5",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.16",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}